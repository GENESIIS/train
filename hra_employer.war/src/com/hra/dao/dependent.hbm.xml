<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="Dependent" table="dependent">
		<meta attribute="class-description">
			This class contains the Dependent detail.
		</meta>
		<id name="dependentTblID" type="int" column="dependentTblID">
			<generator class="native" />
		</id>
		<property name="dependentID" column="ID" type="string" />
		<property name="dependentName" column="NAME" type="string" />
		<property name="dependentDOB" column="DOB" type="date" />
		<property name="employeeID" column="EMPLOYEE_FK_ID" type="string" />
		<property name="updatedBy" column="UPDATED_BY" type="string" />
		<property name="updatedOn" column="UPDATED_ON" type="date" />
	</class>
</hibernate-mapping>