<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="Dependent" table="dependent">
		<meta attribute="class-description">
			This class contains the Dependent detail.
		</meta>
		<id name="dependentTblID" type="int" column="dependentTblID">
			<generator class="native" />
		</id>
		<property name="dependentID" column="dependentID" type="string" />
		<property name="dependentName" column="dependentName" type="string" />
		<property name="dependentDOB" column="dependentDOB" type="date" />
		<property name="employeeID" column="employeeID" type="string" />
		<property name="updatedBy" column="updatedBy" type="string" />
		<property name="updatedOn" column="updatedOn" type="date" />
	</class>
</hibernate-mapping>