			<module name="org.apache.commons.fileupload" />
			<module name="org.apache.commons.io" />
			<module name="org.apache.commons.lang3" />
			<module name="org.apache.commons.validator" />
			<module name="org.apache.lucene" />
			<module name="com.lowagie.itext" />
			<module name="net.sf.jasperreports" />
			<module name="com.google.gson" />
			
		</dependencies>
	</sub-deployment>
	<sub-deployment name="sdb_careers.war">
		<dependencies>
			<module name="org.apache.commons.fileupload" />
			<module name="org.apache.commons.io" />
			<module name="org.apache.commons.lang3" />
			<module name="com.google.gson" />
		</dependencies>
	</sub-deployment>
	<sub-deployment name="sdb_classified.jar">
		<dependencies>
			<module name="org.apache.commons.fileupload" />
			<module name="org.apache.commons.io" />
			<module name="com.lowagie.text" />
			<module name="com.google.gson" />
			<!--add twitter4j-core-2.2.6-SNAPSHOT.jar -->
			<module name="org.apache.commons.lang3"/><!-- merged in from release 294 sha 917b431-->
		</dependencies>
	</sub-deployment>
	<sub-deployment name="sdb_classified.war">
		<dependencies>
			<module name="org.apache.commons.fileupload" />
			<module name="org.apache.commons.io" />
			<module name="org.apache.commons.lang3" />
			<module name="com.lowagie.text" />
			<!--add poi-3.8-beta5-20111217.jar -->
		</dependencies>
	</sub-deployment>
	
	<sub-deployment name="sdb_public.jar">
		<dependencies>
			<module name="org.quartz" /> 	<!--  20150329 quartz support -->
			<module name="org.apache.commons.lang3" />
		</dependencies>
	</sub-deployment>
	
	<sub-deployment name="sdb_public.war">
